---
const response = await fetch('https://jsonplaceholder.typicode.com/users');
const data = await response.json();
console.log(data);
---

<table>
    <th>Name</th>
    <th>Username</th>
    <th>Email</th>
    {
        data.map((item) => (
            <tr>
                <td>{item.name}</td>
                <td>{item.username}</td>
                <td>{item.email}</td>
            </tr>
        ))
    }
</table>
